{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "linterOptions": {
      "exclude": [
        "node_modules/**/*.ts"
      ]
    },
    "rules": {
      "no-console": false,
      "object-literal-sort-keys" : false,
      "strictNullChecks" : false,
      "no-conditional-assignment" : false
    }
  }
  